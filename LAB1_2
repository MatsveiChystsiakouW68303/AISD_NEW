negative_count = 0
n = int(input("Podaj liczbę elementów ciągu: "))

if n <= 0:
    print("Nieprawidłowa liczba elementów ciągu.")
else:
    i = 0
    while i < n:
        x = int(input("Podaj kolejną liczbę: "))
        if x < 0:
            negative_count += 1
        i += 1

    print("Liczba ujemnych liczb w ciągu: ", negative_count)
